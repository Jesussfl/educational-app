{
   "expo": {
      "androidStatusBar": {
         "translucent": true
      },
      "packagerOpts": {
         "config": "metro.config.js",
         "sourceExts": ["js", "jsx", "json", "wasm", "svg"]
      },
      "name": "Finex",
      "slug": "client",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/finex-icon.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "./assets/finex-splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "package": "dev.jeflr.educationalApp",
         "googleServicesFile": "./google-services.json",
         "adaptiveIcon": {
            "foregroundImage": "./assets/finex-icon.png",
            "backgroundColor": "#ffffff"
         },
         "intentFilters": [
            {
            "action": "VIEW",
            "autoVerify": true,
            "data": [
                {
                "scheme": "https",

                "host": "finex.com",
                "pathPrefix": "/records"
                }
            ],
            "category": ["BROWSABLE", "DEFAULT"]
            }
        ]
      },
      "web": {
         "favicon": "./assets/favicon.png"
      },
      "extra": {
         "eas": {
            "projectId": "9a6eb660-a6a5-4a3b-9b07-ec33c628a04c"
         }
      },
      "scheme": "finex"
   }
}
